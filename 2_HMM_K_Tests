addpath(genpath('Pls'))
FO_data = K_testsDiadiDS.Metrics.FO;

for k = 4
    tempdata = FO_data{k};
    indata = cell(7,1);
    task_dex = 1:3:21;
    for task = 1:7
        temp_task = tempdata(task_dex(task):task_dex(task)+2);
        temp_task = cat(3,temp_task{:});
        indata{task} = mean(temp_task,3);
    end
end
clear i k numsubs tempdata option K_res

%%
plotdata = nan(6,7);
for i = 1:7
    plotdata(:,i) = mean(indata{i});
end
clear i

